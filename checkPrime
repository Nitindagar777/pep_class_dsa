#include<bits/stdc++.h>
using namespace std;

bool checkPrime(int n){
    if(n<2) return false;
    bool val = true;
    for(int i=2; i*i<=n; i++){
        if(n % i == 0){
            val = false;
            break;
        }
    }
    return val;
}

void printPrime(int n){
    for(int i=2; i<=n; i++){
        if(checkPrime(i)){
            cout << i << " ";
        }
    }
}

int main(){

    int n = 7;
    printPrime(n);

    return 0;
}
